<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="strutsbase.UsersGroup" table="UsersGroups">
     <id
        name="id"
        type="integer"
        column="id"
        length="254"
       >
        <generator class="assigned" />
    </id>    
    <property
        name="label"
        type="string"
        column="label"
        length="256"
    />    
    
</class>
</hibernate-mapping>